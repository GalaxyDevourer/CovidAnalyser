<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="250.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingsController">
   <children>
      <Button fx:id="folderChooserButton" layoutX="149.0" layoutY="47.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="86.0" text="Choose Folder" />
      <TextField fx:id="folderPathText" editable="false" layoutX="13.0" layoutY="47.0" prefHeight="22.0" prefWidth="116.0" promptText="folder path" />
      <Label layoutX="13.0" layoutY="87.0" prefHeight="22.0" prefWidth="224.0" text="Which algorithm will you use?" />
      <ComboBox fx:id="algoChooserList" layoutX="13.0" layoutY="117.0" prefHeight="22.0" prefWidth="224.0" />
      <Label layoutX="13.0" layoutY="154.0" prefHeight="22.0" prefWidth="116.0" text="Number of clusters:" />
      <Spinner fx:id="clustersChooserSpinner" layoutX="149.0" layoutY="154.0" prefHeight="22.0" prefWidth="86.0" />
      <Label layoutX="14.0" layoutY="14.0" prefHeight="22.0" prefWidth="224.0" text="Select a folder to save the results." />
      <Button fx:id="startClusteringButton" layoutX="52.0" layoutY="207.0" mnemonicParsing="false" onMouseClicked="#startClustering" prefHeight="22.0" prefWidth="146.0" text="Start clustering" />
   </children>
</AnchorPane>
